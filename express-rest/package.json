    "typescript-is": "^0.18.3",
    "ws": "^8.2.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.13",
    "@types/express-ws": "^3.0.1",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/lodash": "^4.14.176",
    "@types/node": "^16.11.4",
    "@types/ws": "^8.2.0",
